<?xml version="1.0"?>
<metaproxy xmlns="http://indexdata.com/metaproxy" version="1.0">
  <dlpath>../src</dlpath>
  <start route="start"/>
  <routes>
    <route id="start">
      <filter type="frontend_net">
	<threads>10</threads>
	<port>@:9000</port>
	<message>FN</message>
      </filter>
      <filter type="log">
        <message>log</message>
      </filter>
      <include src="triplestore.xml"/>
      <filter type="log">
	<message>http</message>
	<category apdu="true"/>
      </filter>
      <filter type="http_client">
        <x-forwarded-for>true</x-forwarded-for>
      </filter>
      <filter type="bounce"/>
    </route>
  </routes>
</metaproxy>

